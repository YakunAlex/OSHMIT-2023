#include <iostream>
using namespace std;

int main() {
	// даны три числа, нужно вывести наибольшее
	int x, y, z;
	cin >> x >> y >> z;

	if (x >= y && x >= z)  { 
		// && - логическое И
		// || - логическое ИЛИ
		// ! - логическое НЕ
		cout << x;
	}
	else if (y >= x && y >= z) {
		cout << y;
	}
	else {
		cout << z;
	}
	// стандартная конструкция if - else if - .. - else if - else
	// гарантирует что мы войдем только в одно из таких тел
}
